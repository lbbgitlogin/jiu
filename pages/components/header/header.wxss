/* 小程序自定义头部bar */
.navigationBar {
    width: 100%;
  }
  .navigationBar > view {
    width: 100%;
    display: flex;
    align-items: center;
  }
  .navigationBar text {
    height: 100%;
    color: #fff;
    font-size: 36rpx;
    height: 36rpx;
    line-height: 1;
  }
  .navigationBar .title {
    width: 100%;
    text-align: center;
  }
  .navigationBar .back {
    position: absolute;
    left: 30rpx;
    top: 0;
    width: 200rpx;
    height: 100%;
    display: flex;
    align-items: center;
  }
  .navigationBar .back .ic {
    width: 16rpx;
    height: 30rpx;
    background-image: url("data:image/png;base64,");
  }
  .navigationBar .back text {
    margin-left: 10rpx;
    color: #000;
  }